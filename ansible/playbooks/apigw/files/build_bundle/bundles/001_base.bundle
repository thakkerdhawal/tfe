      &lt;xs:extension base="xs:nonNegativeInteger"&gt;&#13;
        &lt;xs:anyAttribute namespace="##other"/&gt;&#13;
      &lt;/xs:extension&gt;&#13;
    &lt;/xs:simpleContent&gt;&#13;
