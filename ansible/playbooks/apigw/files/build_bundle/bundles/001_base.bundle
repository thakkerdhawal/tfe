  &lt;!-- //////////////////// WS-Addressing //////////////////// --&gt;&#13;
  &lt;!-- Endpoint reference --&gt;&#13;
  &lt;xs:element name="EndpointReference" type="wsa:EndpointReferenceType"/&gt;&#13;
