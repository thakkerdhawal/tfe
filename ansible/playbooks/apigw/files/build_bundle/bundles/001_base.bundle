            <l7:Resource>
                <l7:ResourceDocument id="0000000000000000fffffffffffffff9" version="0">
                    <l7:Resource sourceUrl="http://www.w3.org/2001/XMLSchema.dtd" type="dtd">&lt;!-- DTD for XML Schemas: Part 1: Structures
